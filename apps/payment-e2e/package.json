{
  "name": "@nestjs-microservices/payment-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@nestjs-microservices/payment"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/payment-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@nestjs-microservices/payment:build",
          "@nestjs-microservices/payment:serve"
        ]
      }
    }
  }
}
